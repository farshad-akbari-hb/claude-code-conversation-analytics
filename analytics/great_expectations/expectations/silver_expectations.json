{
  "data_asset_type": null,
  "expectation_suite_name": "silver_expectations",
  "expectations": [
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "conversation_id"
      },
      "meta": {
        "notes": {
          "format": "markdown",
          "content": "Primary key in staging models must not be null"
        }
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "entry_type",
        "value_set": ["user", "assistant", "tool_use", "tool_result", "unknown"]
      },
      "meta": {
        "notes": {
          "format": "markdown",
          "content": "Entry types must be valid (includes 'unknown' for edge cases)"
        }
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "task_category",
        "value_set": ["bug_fix", "feature", "refactor", "testing", "documentation", "review", "other"]
      },
      "meta": {
        "notes": {
          "format": "markdown",
          "content": "Task category must be a valid classification"
        }
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "time_of_day",
        "value_set": ["morning", "afternoon", "evening", "night"]
      },
      "meta": {
        "notes": {
          "format": "markdown",
          "content": "Time of day must be a valid period"
        }
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
